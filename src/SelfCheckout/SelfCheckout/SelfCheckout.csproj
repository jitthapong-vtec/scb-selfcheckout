<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>pdbonly</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Images\bg.png" />
    <None Remove="Resources\Images\header_logo.svg" />
    <None Remove="Resources\Images\Icons\check_box_active.svg" />
    <None Remove="Resources\Images\Icons\check_box_normal.svg" />
    <None Remove="Resources\Images\landing_bg.png" />
    <None Remove="Resources\Images\line_seperator.png" />
    <None Remove="Resources\order_list.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Acr.UserDialogs" Version="7.0.35" />
    <PackageReference Include="MagicGradients" Version="1.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Rg.Plugins.Popup" Version="1.2.0.223" />
    <PackageReference Include="Xamarin.Essentials" Version="1.3.1" />
    <PackageReference Include="Xamarin.FFImageLoading" Version="2.4.11.982" />
    <PackageReference Include="Xamarin.FFImageLoading.Forms" Version="2.4.11.982" />
    <PackageReference Include="Xamarin.FFImageLoading.Svg.Forms" Version="2.4.11.982" />
    <PackageReference Include="Xamarin.Forms" Version="4.4.0.991640" />
    <PackageReference Include="Xamarin.Forms.Visual.Material" Version="4.4.0.991640" />
    <PackageReference Include="ZXing.Net.Mobile.Forms" Version="2.4.1" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Images\bg.png" />
    <EmbeddedResource Include="Resources\Images\header_logo.svg" />
    <EmbeddedResource Include="Resources\Images\Icons\check_box_active.svg" />
    <EmbeddedResource Include="Resources\Images\Icons\check_box_normal.svg" />
    <EmbeddedResource Include="Resources\Images\landing_bg.png" />
    <EmbeddedResource Include="Resources\Images\line_seperator.png" />
    <EmbeddedResource Include="Resources\order_list.json" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\AppResources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>AppResources.resx</DependentUpon>
    </Compile>
    <Compile Update="Views\Components\UserAuthenView.xaml.cs">
      <DependentUpon>UserAuthenView.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\OrderItemView.xaml.cs">
      <DependentUpon>OrderItemView.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\SummaryView.xaml.cs">
      <DependentUpon>SummaryView.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\CurrencySelectionView.xaml.cs">
      <DependentUpon>CurrencySelectionView.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\ShoppingCartView.xaml.cs">
      <DependentUpon>ShoppingCartView.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Controls\GradientBackground.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\AppResources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>AppResources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\ActionBarView.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\ActivityIndicatorView.xaml">
      <Generator>MSBuild:Compile</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\AuthorizationView.xaml">
